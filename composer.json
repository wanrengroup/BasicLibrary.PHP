{
  "name": "wanren/basiclibrary.php",
  "description": "WRJT's php basic class library.",
  "homepage": "https://www.wanren.com/",
  "keywords": [
    "php",
    "basic library",
    "codecs"
  ],
  "type": "string",
  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.4.0",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-bcmath": "*",
    "ext-iconv": "*",
    "ext-gd": "*",
    "monolog/monolog": "^2.9.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "9.*",
    "wanren/tp32-phpunit-tool": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "WanRen\\Test\\": "tests",
      "WanRen\\": "src"
    },
    "files": [
      "src/Common/functions.php"
    ]
  }
}
